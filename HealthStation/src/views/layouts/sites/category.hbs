{{#section 'title'}}
<title>Category</title>
{{/section}}

{{#section 'style'}}
<link href="/css/home.css" rel="stylesheet" />
{{/section}}

<section class="section-sm pt-0">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="search-result bg-gray">
                    <h2>Kết quả cho "{{mainCategory.name}}"</h2>
                    <p>123 Results ...</p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="category-sidebar">
                    <div class="widget category-list">
                        <h4 class="widget-header">All Category</h4>
                        <ul class="category-list">
                            {{!-- <li><a href="/category/5">Gia dụng, tiện ích <span>93</span></a></li>
                            <li><a href="/category/5">Lương thực, thực phẩm <span>233</span></a></li>
                            <li><a href="/category/5">Dành cho bé <span>183</span></a></li>
                            <li><a href="/category/5">Vật dụng y tế <span>343</span></a></li> --}}
                            {{#each categories}}
                            <li><a href="/category/{{this.category_id}}">{{this.name}}<span>233</span></a></li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-md-9">
                <div class="category-search-filter">
                    <div class="row">
                        <div class="col-md-6">
                            <select class="custom-select mt-lg-1 mt-md-2">
                                <option>Hiển thị</option>
                                <option value="">Giá thấp</option>
                                <option value="">Giá cao</option>
                            </select>
                            <div class="form-group">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="product-grid-list">
                    <div class="row mt-30">
                        {{!-- <div class="col-sm-12 col-lg-4 col-md-6">
                            {{> packetPartial
                            imgSrc='data:image/jpeg;base64,'
                            name=data.name description=data.description}}
                        </div>
                        <div class="col-sm-12 col-lg-4 col-md-6">
                            {{> packetPartial
                            imgSrc='data:image/jpeg;base64,'
                            name=data.name description=data.description}}
                        </div>
                        <div class="col-sm-12 col-lg-4 col-md-6">
                            {{> packetPartial
                            imgSrc='data:image/jpeg;base64,'
                            name=data.name description=data.description}}
                        </div> --}}
                        {{#each packages}}
                        <div class="col-sm-12 col-lg-4 col-md-6">
                            {{> packetPartial
                            imgSrc='data:image/jpeg;base64,'
                            name=this.name 
                            id=this.package_id
                            category=../mainCategory
                            }}
                        </div>
                        {{/each}}
                    </div>
                </div>
                <div class="pagination justify-content-center">
                    <nav aria-label="Page navigation example">
                        <ul class="pagination">
                            <li class="page-item">
                                <a class="page-link" href="#" aria-label="Previous">
                                    <span aria-hidden="true">«</span>
                                    <span class="sr-only">Previous</span>
                                </a>
                            </li>
                            <li class="page-item"><a class="page-link" href="#">1</a></li>
                            <li class="page-item active"><a class="page-link" href="#">2</a></li>
                            <li class="page-item"><a class="page-link" href="#">3</a></li>
                            <li class="page-item">
                                <a class="page-link" href="#" aria-label="Next">
                                    <span aria-hidden="true">»</span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</section>